{
    "name": "AVNET_WNC14A2A",
    "config": {
        "baudrate": {
            "help": "Serial connection baud rate",
            "value": 115200
        },
        "cts": {
            "help": "WNC doesn't utilize RTS/CTS but the pin is connected",
            "value": "D10"
        },
        "debug": {
            "help": "Enable WNC driver debug messages.",
            "value": false
        },
        "debug_setting": {
            "help": "Bit value 1 and/or 2 enable WncController debug output, bit value 4 enables mbed driver debug output.",
            "value": "0x0c"
        },
        "lvltranslator": {
            "help": "Module Level Translator enable Line",
            "value": "D9"
        },
        "power_on": {
            "help": "Module Power ON Line",
            "value": "D2"
        },
        "provide-default": {
            "help": "Provide as default CellularDevice [true/false]",
            "value": false
        },
        "reset": {
            "help": "Module Reset Line",
            "value": "D8"
        },
        "rx": {
            "help": "RX pin for serial connection. D0 assumed if Arduino Form Factor, needs to be set/overwritten otherwise.",
            "value": "D0"
        },
        "rx_boot_sel": {
            "help": "Module RX Boot Select Line",
            "value": "D1"
        },
        "tx": {
            "help": "TX pin for serial connection. D1 assumed if Arduino Form Factor, needs to be set/overwritten otherwise.",
            "value": "D1"
        },
        "wakeup": {
            "help": "Module Wakeup Line",
            "value": "D6"
        }
    }
}